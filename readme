dd if=/dev/zero of=/var/swap bs=1024 count=1024000
mkswap -f /var/swap
swapon /var/swap
echo "/var/swap swap swap defaults 0 0" >> /etc/fstab


curl --proxy socks5h://127.0.0.1:40000 https://www.cloudflare.com/cdn-cgi/trace 
